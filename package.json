    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^16.11.1",
    "@types/tap": "^15.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.2.0",
